<style>
	footer {
		padding: 2em 1em 6em 1em;
		background: linear-gradient(var(--navigation-gradient)) no-repeat;
		color: var(--navigation);
		text-align: center;
	}
	:global(.gallery-layout) footer {
		color: white;
		padding:1rem 0;
		background: rgb(119, 160,118);
		font-size: 1rem;
	}
	ul {
		padding-left: 0;
	}
	.flexy {
		display:flex;
		justify-content: center;
		flex-wrap: wrap;
	}
	.bottom-nav li {
		list-style: none;
	}
	.bottom-nav a {
		text-decoration: none;
		font-weight: 700;
		color: var(--navigation);
		margin: 0 1rem;
	}
	:global(.gallery-layout) .bottom-nav a {
		color: white;
		&:hover {
			color: darkolivegreen;
		}
	}
	.social-links {
		display: flex;
		justify-content: center;
		gap: 1em;
		margin-top: 1em;
	}
	.social-links a {
		text-decoration: none;
		color: rgb(var(--gray));
	}
	.social-links a:hover {
		color: rgb(var(--gray-dark));
	}
</style>
